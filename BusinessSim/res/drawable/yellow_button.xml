<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#F9F96D" />
      <stroke
          android:width="2dp"
          android:color="#FFC300" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item><layer-list>
      
      <item android:left="8dp" android:top="8dp"><shape>
                        <corners android:radius="15dp" />

                        <solid android:color="#165900" />
      </shape></item>
      
    <item android:bottom="2dp" android:left="2dp"><shape>
      <gradient
          android:startColor="#F7F745"
          android:endColor="#F8FC02"
          android:angle="270" />
      <stroke
          android:width="2dp"
          android:color="#FFC300" />
      <corners
          android:radius="15dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
      
    </shape></item></layer-list>
  </item>
</selector>